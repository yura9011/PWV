{
  "searchDirectory": ".",
  "fileExtensions": [
    "cs", "json", "md", "txt", "yaml", "yml"
  ],
  "excludePatterns": [
    "**/node_modules/**",
    "**/.git/**",
    "**/Library/**",
    "**/Temp/**",
    "**/Logs/**",
    "**/obj/**",
    "**/bin/**",
    "**/*.meta"
  ],
  "chunkSize": 30,
  "maxFileSize": 2097152,
  "maxResults": 10,
  "enableCache": true,
  "verbose": false
}
